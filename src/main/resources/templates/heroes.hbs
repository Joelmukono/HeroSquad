{{#partial "content"}}


    <div class="jumbotron">
        <h1 class="squad">List of heroes for {{oneSquad.name}} squad</h1>

    </div>


    <h3 class="char">Hero characteristics</h3>

    <ol>
        {{#each oneHero}}
            <li>
                <h3 id="namehero">{{name}}</h3>
                <h3 id ="agehero">{{age}}</h3>
                <h3 id="powerhero">{{power}}</h3>
                <h3 id="weaknesshero">{{weakness}}</h3>
            </li>
        {{/each}}
    </ol>
{{/partial}}

{{> layout.hbs}}